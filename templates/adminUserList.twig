{% include 'header.twig' %}
    {% include 'navbar.twig' %}
    
    {% include 'adminMenu.twig' with {'tab' : 2 } %}

    <div class="container-fluid content">

    <div class="container">
        <div class="table-responsive">
            <table class="table table-hover" id="myTable">
                <tr>
                    <th>#</th>
                    <th>Ambasador</th>
                    <th>Email</th>
                    <th>PESEL</th>
                    <th>Telefon</th>
                    <th>Rejestracja</th>
                    <th>Kod</th>
                </tr>
                {% for user in users %}
                <tr>
                    <td>{{ user.ID }}</td>
                    <td>{{ user.Name }}</td>
                    <td>{{ user.Email }}</td>
                    <td>{{ user.Pesel }}</td>
                    <td>{{ user.Phone }}</td>
                    <td>{{ user.Created }}</td>
                    <td>{{ user.UserKey }}</td>                    
                </tr>
                {% endfor %}
            </div>
        </table>
    </div><!-- /.container   -->

